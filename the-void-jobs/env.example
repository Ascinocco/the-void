# Environment Configuration
NODE_ENV=development

# Job Scheduling
JOB_SCHEDULE=0 * * * *

# API Configuration
API_BASE_URL=https://api.example.com
API_KEY=your-api-key-here

# Logging
DEBUG=false

# Add your environment variables here
# DATABASE_URL=
# REDIS_URL=
# SMTP_HOST=
# SMTP_PORT=
# SMTP_USER=
# SMTP_PASS=

# Tavily Search API
TAVILY_API_KEY=your-tavily-api-key-here

# Bluesky API Configuration
# Option 1: Use app password authentication (requires creating app password in Bluesky settings)
BLUESKY_IDENTIFIER=your-bluesky-handle-or-email
BLUESKY_PASSWORD=your-bluesky-app-password

# Option 2: Use direct access token (if you have one)
BLUESKY_ACCESS_TOKEN=your-bluesky-access-token

# Mastodon API Configuration (optional, may improve search results)
MASTODON_INSTANCE=https://mastodon.social
MASTODON_ACCESS_TOKEN=your-mastodon-access-token
